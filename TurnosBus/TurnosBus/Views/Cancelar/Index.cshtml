
@{
    ViewBag.Title = "Cancelar";
}
<style>
    div#options h3#option4 {
        opacity: 1;
    }
</style>
<div id="appbar">
    <img src="~/Content/img/espe.png" alt="" id="image">
    <button id="salirbutton" onclick="onClickSalir()">Salir</button>
</div>
<div id="container">
    <div id="title">
        <h1 id="texttitle">Horarios y Unidades</h1>
    </div>
    <div id="options">
        <h3 id="option1">Horarios y Unidades</h3>
        <h3 id="option2">Reserva un Turno</h3>
        <h3 id="option3">Verificar Disponibilidad</h3>
        <h3 id="option4">Cancelar Turno</h3>
    </div>
    <div id="content">
        <div id="datetime">
            <h3 id="datetext">Fecha: XX/XX/XXXX</h3>
            <h3 id="hourtext">Hora: XX:XX</h3>
        </div>
        <div id="tablecontainer">

        </div>
    </div>
</div>
